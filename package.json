{
  "name": "prompt-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit --pretty",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ],
    "*.{js,jsx,ts,tsx,css,md,mdx,json,yml,yaml,mjs,cjs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.0",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.11.30",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "postcss": "8.4.35",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  }
}
